{
  "name": "instagram-automation-dashboard",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "backend": "cd backend && node server.js",
    "backend:dev": "cd backend && npm run dev",
    "backend:install": "cd backend && npm install",
    "tunnel:start": "cloudflared tunnel --url http://localhost:3001",
    "tunnel:named": "cloudflared tunnel run instagram-automation-backend",
    "tunnel:stop": "pkill -f cloudflared",
    "tunnel:status": "ps aux | grep cloudflared",
    "dev:production": "concurrently \"npm run backend\" \"npm run tunnel:start\" \"npm run dev\"",
    "test:webhook": "curl https://instagram-backend.888intelligenceautomation.in/health",
    "test:meta": "curl -X GET \"https://instagram-backend.888intelligenceautomation.in/webhook/instagram?hub.mode=subscribe&hub.verify_token=instagram_automation_cf_token_2024&hub.challenge=hello\"",
    "test:backend": "curl http://localhost:3001/health",
    "setup": "npm install && npm run backend:install",
    "check:cloudflared": "cloudflared --version",
    "check:backend": "ls -la backend/",
    "clean": "rm -rf node_modules backend/node_modules",
    "fresh-install": "npm run clean && npm install && npm run backend:install"
  },
  "dependencies": {
    "@babel/core": "^7.28.0",
    "@tanstack/react-query": "^5.84.0",
    "@types/react": "^18.2.0",
    "axios": "^1.10.0",
    "debug": "^4.4.1",
    "dotenv": "^16.6.1",
    "lucide-react": "^0.525.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.7.1",
    "recharts": "^2.7.2",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.14",
    "concurrently": "^9.0.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.1",
    "nodemon": "^3.1.10",
    "postcss": "^8.4.24",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0",
    "cloudflared": "2025.7.0" 
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/instagram-automation-dashboard.git"
  },
  "keywords": [
    "instagram",
    "automation",
    "react",
    "vite",
    "typescript",
    "cloudflare",
    "webhook",
    "meta-api"
  ],
  "author": "",
  "license": "MIT",
  "description": "Instagram automation dashboard with Meta API integration and Cloudflare tunneling"
}