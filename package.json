{
  "name": "instagram-automation-dashboard",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "// ============ DEVELOPMENT SCRIPTS ============": "",
    "dev": "vite",
    "build": "npx vite build --config ./vite.config.ts",
    "preview": "vite preview",
    "// ============ BACKEND SCRIPTS ============": "",
    "backend": "cd backend && node server.js",
    "backend:dev": "cd backend && npm run dev",
    "backend:install": "cd backend && npm install",
    "// ============ UNIFIED TUNNEL MANAGEMENT (NEW) ============": "",
    "tunnel": "node scripts/tunnel-manager-unified.js start --mode dual",
    "tunnel:single": "node scripts/tunnel-manager-unified.js start --mode single",
    "tunnel:dual": "node scripts/tunnel-manager-unified.js start --mode dual",
    "tunnel:status": "node scripts/tunnel-manager-unified.js status",
    "tunnel:stop": "node scripts/tunnel-manager-unified.js stop",
    "tunnel:test": "node scripts/tunnel-manager-unified.js test",
  
    "// ============ TESTING ENDPOINTS ============": "",
    "test:backend": "curl http://localhost:3001/health",
    "test:webhook": "curl https://api.888intelligenceautomation.in/health",
    "test:meta": "curl -X GET \"https://api.888intelligenceautomation.in/webhook/instagram?hub.mode=subscribe&hub.verify_token=instagram_automation_cf_token_2024&hub.challenge=hello\"",
    "test:n8n": "curl https://api.888intelligenceautomation.in/webhook/n8n-status",
    "test:routes": "curl https://api.888intelligenceautomation.in/webhook/test",
    "tunnel:test-api": "curl https://api.888intelligenceautomation.in/health",
    "tunnel:test-db": "curl -I https://db-secure.888intelligenceautomation.in/rest/v1/",
    "test:full": "npm run test:backend && npm run test:webhook && npm run test:n8n",
    "// ============ COMBINED DEVELOPMENT MODES ============": "",
    "dev:production": "concurrently \"npm run backend\" \"npm run tunnel:single\" \"npm run dev\"",
    "dev:secure": "concurrently \"npm run backend\" \"npm run tunnel:dual\" \"npm run dev\"",
    "dev:full": "concurrently \"npm run backend\" \"npm run tunnel\" \"npm run dev\"",
    "dev:local": "concurrently \"npm run backend\" \"npm run dev\"",
    "// ============ SETUP & INSTALLATION ============": "",
    "setup": "npm install && npm run backend:install",
    "setup:tunnel-b": "bash deploy-tunnel-b.sh",
    "setup:supabase": "npm install @supabase/supabase-js && cd backend && npm install @supabase/supabase-js",
    "setup:unified": "node scripts/setup-unified-tunnel.js",
    "// ============ ENVIRONMENT MANAGEMENT ============": "",
    "env:check": "node -e \"const fs=require('fs'); console.log('.env exists:', fs.existsSync('.env')); console.log('.env.tunnel-single exists:', fs.existsSync('.env.tunnel-single')); console.log('.env.tunnel-dual exists:', fs.existsSync('.env.tunnel-dual'));\"",
    "env:backup": "node -e \"const fs=require('fs'); const date=new Date().toISOString().replace(/[:.]/g,'-'); fs.existsSync('.env') && fs.copyFileSync('.env', `.env.backup.${date}`) && console.log(`Backup created: .env.backup.${date}`)\"",
    "env:list-backups": "ls -la .env.backup.* 2>/dev/null || echo 'No backups found'",
    "env:template:view": "cat .env.tunnel-dual 2>/dev/null || echo 'No dual template found. Run: npm run tunnel:dual'",
    "// ============ UTILITY & MAINTENANCE ============": "",
    "check:cloudflared": "cloudflared --version",
    "check:backend": "ls -la backend/",
    "check:ports": "lsof -i :3001 || echo 'Port 3001 is available'",
    "check:all": "npm run check:cloudflared && npm run check:backend && npm run check:ports",
    "clean": "rm -rf node_modules backend/node_modules",
    "clean:tunnels": "rm -f tunnel-status.json tunnel-test-results.json .env.tunnel-*",
    "fresh-install": "npm run clean && npm install && npm run backend:install",
    "// ============ DATABASE MANAGEMENT ============": "",
    "db:types": "npx supabase gen types typescript --project-id uromexjprcrjfmhkmgxa > src/lib/database.types.ts",
    "db:status": "npx supabase status",
    
    "// ============ PRODUCTION DEPLOYMENT ============": "",
    "start": "npm run backend",
    "start:tunnels": "npm run tunnel",
    "deploy:pm2": "pm2 start ecosystem.config.js",
    "deploy:check": "pm2 status",
    "// ============ MIGRATION HELPERS (Temporary - Remove after migration) ============": "",
    "migrate:backup-old": "mkdir -p scripts/deprecated && mv scripts/tunnel-manager-cf.js scripts/deprecated/ 2>/dev/null; mv scripts/tunnel-manager-dual.js scripts/deprecated/ 2>/dev/null || echo 'Old scripts already moved or not found'",
    "migrate:verify": "npm run env:check && npm run tunnel:status",
    "migrate:test": "npm run tunnel:test && npm run test:full"
  },
  "dependencies": {
    "@babel/core": "^7.28.0",
    "@radix-ui/react-focus-scope": "^1.1.7",
    "@supabase/supabase-js": "^2.56.1",
    "@tanstack/react-query": "^5.85.6",
    "axios": "^1.10.0",
    "cli": "^1.0.1",
    "debug": "^4.4.1",
    "dotenv": "^16.6.1",
    "framer-motion": "^12.23.12",
    "lucide-react": "^0.525.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.6.0",
    "react-router-dom": "^7.7.1",
    "recharts": "^2.7.2",
    "supabase": "^2.39.2",
    "tailwind": "^4.0.0",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@playwright/mcp": "^0.0.47",
    "@playwright/test": "^1.56.1",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.3.7",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.14",
    "concurrently": "^9.0.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.1",
    "nodemon": "^3.1.10",
    "postcss": "^8.4.24",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0",
    "cloudflared": "2025.7.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/instagram-automation-dashboard.git"
  },
  "keywords": [
    "instagram",
    "automation",
    "react",
    "vite",
    "typescript",
    "cloudflare",
    "webhook",
    "meta-api",
    "supabase",
    "n8n-workflows"
  ],
  "author": "",
  "license": "MIT",
  "description": "Instagram automation dashboard with Meta API integration, Supabase database, and Cloudflare zero-trust tunneling"
}
